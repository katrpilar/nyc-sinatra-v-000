<h1>Eidt This Landmark</h1>
<form action="/landmarks/<%=@landmark.id%>" method="POST">
  <input id="hidden" type="hidden" name="_method" value="patch"></input>
  <label for="name">Name</label>
  <input type="text" name="landmark[name]" id="name" value="<%=@landmark.name%>">

  <label for="year_completed">Year</label>
  <input type="text" name="landmark[year_completed]" id="year_completed" value="<%=@landmark.year_completed%>">


  <input type="submit" name="Edit Landmark" value="Edit Landmark">
</form>
